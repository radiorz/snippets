{
  "name": "snippets",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "workspaces": {
    "packages": [
      "vscode-extension"
    ]
  },
  "scripts": {
    "install:vscode": "yarn workspace tikkhun-vscode-snippets install",
    "gen": "node ./src/gen.js",
    "gen:vscode": "yarn gen  && node ./src/gen_extension.js",
    "push:local": "powershell ./src/publish.ps1",
    "release:local": "yarn gen && yarn push:local",
    "build:vscode": "yarn gen:vscode && yarn workspace tikkhun-vscode-snippets build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/radiorz/snippets.git"
  },
  "categories": [
    "Snippets"
  ],
  "keywords": [],
