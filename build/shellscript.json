{
  "shellscript_tar_out": {
    "prefix": [
      "!tarout"
    ],
    "description": "\ntar -zxvf ${1:压缩包名}.tar.gz -C ${2:.}\n",
    "body": [
      "tar -zxvf ${1:压缩包名}.tar.gz -C ${2:.}"
    ]
  },
  "shellscript_tar": {
    "prefix": [
      "!tar"
    ],
    "description": "\ntar -zcvf ${1:$CURRENT_MONTH-$CURRENT_DATE $CURRENT_HOUR:$CURRENT_MINUTE:$CURRENT_SECOND}.tar.gz ${2:*} \n",
    "body": [
      "tar -zcvf ${1:$CURRENT_MONTH-$CURRENT_DATE $CURRENT_HOUR:$CURRENT_MINUTE:$CURRENT_SECOND}.tar.gz ${2:*}"
    ]
  }
}